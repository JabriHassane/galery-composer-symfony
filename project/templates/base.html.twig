        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/css2?family=Palette+Mosaic&family=Style+Script&display=swap" rel="stylesheet">
